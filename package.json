{
  "name": "yafa-prompt-engine",
  "version": "4.0.0",
  "description": "Production-ready, template-free, cartridge-driven prompt orchestration engine",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "docker:build": "docker build -t yafa-engine .",
    "docker:run": "docker run -p 3001:3001 yafa-engine"
  },
  "keywords": [
    "prompt-engineering",
    "llm",
    "ai",
    "cartridge",
    "orchestration",
    "template-free"
  ],
  "author": "YAFA Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-rate-limit": "^7.1.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "express-prometheus-middleware": "^1.2.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "ioredis": "^5.3.2",
    "bullmq": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.1",
    "fs-extra": "^11.2.0",
    "path": "^0.12.7",
    "crypto": "^1.0.1",
    "prom-client": "^14.2.0",
    "openai": "^4.28.0",
    "@anthropic-ai/sdk": "^0.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "eslint": "^8.57.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
