# Server Configuration
PORT=3001
NODE_ENV=development

# LLM Providers
OPENAI_API_KEY=your_openai_api_key_here

# Database
DATABASE_URL=postgresql://yafa:yafa@localhost:5432/yafa?schema=public

# Queue/Cache
REDIS_URL=redis://localhost:6379

# Artifact Storage (MinIO/S3-compatible)
S3_ENDPOINT=http://localhost:9000
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=yafa
S3_SECRET_ACCESS_KEY=yafa12345
S3_BUCKET=yafa-artifacts

# Telemetry (optional)
OTEL_EXPORTER_OTLP_ENDPOINT=
ENABLE_TELEMETRY=false

# ngrok Configuration
NGROK_AUTHTOKEN=your_ngrok_authtoken
NGROK_DOMAIN=your_custom_domain.ngrok.app

# Security
BASIC_AUTH_USER=yafa
BASIC_AUTH_PASS=secure123
